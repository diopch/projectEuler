#include <iostream>
#include <string>

int main()
{
    int result = 0;
    int a = 1;
    int b = 2;
    int c = 0;
    while (b < 4000000)
    {
        if(b%2==0)
        {
            result = result + b;
        }
        c=a;
        a = b;
        b= b+c;
    }
    
  
  std::cout << "result :  " << result << "!\n";
}
